*/This program prints every number from 1 to 100. 
For every number divisable by 3 it prints "Fizz".
For every number divisable by 5 it prints "Buzz".
For every number divisable by both it prints "FizzBuzz".
*/

for(let buildingBlock = 1; buildingBlock <=100 ; buildingBlock++){
  if (buildingBlock % 3 == 0 && buildingBlock % 5 ==0) {
    console.log("FizzBuzz");
  }
  else if (buildingBlock % 3 == 0) {
    console.log("Fizz");
  }
  else if (buildingBlock % 5 == 0) {
    console.log("Buzz");
  }
  else console.log(buildingBlock)
  }

*/ a more clever version is below */
for(let buildingBlock=1; buildingBlock <=100; buildingBlock++) {
  let output = "";
  if (buildingBlock % 3 == 0) output += "Fizz";
  if (buildingBlock % 5 == 0) output += "Buzz";
  console.log(output || n);
}
